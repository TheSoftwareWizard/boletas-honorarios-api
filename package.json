{
  "name": "boletas-honorarios-api",
  "version": "1.0.0",
  "description": "REST API for Chilean professional service invoice (boletas de honorarios) retention data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "type-check": "tsc --noEmit",
    "update-retention": "ts-node src/scripts/updateRetention.ts"
  },
  "keywords": [
    "chile",
    "withholding-tax",
    "boletas-honorarios",
    "api"
  ],
  "author": "Jean Roa Web <https://jeanroa.dev>",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.1",
    "fastify": "^4.26.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "pino-pretty": "^11.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
